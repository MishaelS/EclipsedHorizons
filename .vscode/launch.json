{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug EclipsedHorizons",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/build/bin/EclipsedHorizons.exe",
			"args": [
				"arg1",
				"arg2"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"miDebuggerPath": "C:/path/to/gdb.exe",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "Build Project"
		}
	]
}